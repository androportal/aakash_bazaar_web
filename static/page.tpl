<!DOCTYPE html>
<html>
<head>
	<title>Aakash Bazaar</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="css/bootstrap.min.css" rel="stylesheet" media="screen">
	<link href="css/bootstrap-responsive.min.css" rel="stylesheet" media="screen">
	<link href="css/style.css" rel="stylesheet" media="screen">
</head>

<body>
	<div id="header-wrapper">
		<div id="header-inner">
		<h3 class="pull-left">Aakash Bazaar</h3>
	
		<ul class="nav nav-pills pull-right">
			<li class="active">
			<a href="#">Home</a>
			</li>
			<li><a href="#">Login</a></li>
			<li><a href="#">Register</a></li>
			<li><a href="#">Contact</a></li>
		</ul>

		<div class="clearfix"></div>
		</div> <!-- ./header-inner -->
	</div> <!-- ./header-wrapper -->

	<div id="page-wrapper">
		<div id="page">
			<h3>Example Form</h3>
			<div class="">
				<form>
					<label>Package Name</label>
					<input type="text" class="span6">
					<label>Name of APK</label>
					<input type="text" class="span6">
					<label>License</label>
					<select class="span6">
						<option>GNU GPLv3</option>
						<option>GNU GPLv2</option>
					</select>
					<label>Website</label>
					<input type="text" class="span6">
					<label>Source Code</label>
					<input type="text" class="span6">
					<label>Issue Tracker</label>
					<input type="text" class="span6">
					<label>Summary</label>
					<textarea class="span6"></textarea>
					<label>Description</label>
					<textarea class="span6"></textarea>
					<label>Changelog</label>
					<textarea class="span6"></textarea><br>
					<input type="submit" value="Submit" class="btn btn-primary btn-large span3">
				</form>
			</div>

		</div> <!-- ./page -->
	</div><!-- ./page-wrapper -->

	<div id="footer-wrapper">
		<div class="pull-left">
			&copy; 2013 AakashLabs
		</div>
		<div class="pull-right">
			Designed by Aakash-Team
		</div>
		<div class="clearfix"></div>
	</div> <!-- ./footer-wrapper -->

<script src="http://code.jquery.com/jquery.js"></script>
<script src="js/bootstrap.min.js"></script>
<script type="text/javascript"></script>
</body>
</html>